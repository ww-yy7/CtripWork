#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../../../../../@react-native-community+cli@12.3.2/node_modules/@react-native-community"
else
  export NODE_PATH="$NODE_PATH:$basedir/../../../../../@react-native-community+cli@12.3.2/node_modules/@react-native-community"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@react-native-community+cli@12.3.2/node_modules/@react-native-community/cli/build/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@react-native-community+cli@12.3.2/node_modules/@react-native-community/cli/build/bin.js" "$@"
fi
